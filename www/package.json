{
  "name": "terrahawk-www",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "dev:todo:ssr": "ts-node ./server",
    "dev": "vite",
    "vit:todo:ssr": "vite build && vite build --ssr",
    "serve:todo:ssr": "yarn vit && cross-env NODE_ENV=production ts-node ./server",
    "build:todo:ssr": "yarn vit && vite-plugin-ssr prerender --no-extra-dir",
    "serve": "vite preview",
    "build": "yarn clean && tsc && vite build",
    "deploy:alpha": "yarn build && AWS_PROFILE=jncc-habmon-alpha-admin sls client deploy"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "@tailwindcss/typography": "^0.4.1",
    "@types/express": "^4.17.13",
    "@types/leaflet": "^1.7.5",
    "@types/markdown-it": "^12.2.1",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.3.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "leaflet": "^1.7.1",
    "markdown-it": "^12.2.0",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.1",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-observable": "^2.0.0",
    "rimraf": "^3.0.2",
    "serverless": "^2.59.0",
    "serverless-finch": "^2.6.0",
    "tailwindcss": "^2.2.15",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "vite": "^2.5.2",
    "vite-plugin-ssr": "^0.2.13"
  }
}
